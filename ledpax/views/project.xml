<?xml version="1.0" encoding="utf-8"?>
<odoo>
<template id="project_name_page" name="Projects">
    <t t-call="website.layout">
    <xpath expr="." position="inside">
    	<head>
			<meta charset="utf-8"></meta>
			<meta http-equiv="X-UA-Compatible" content="IE=edge"></meta>
			<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no"></meta>
			<meta name="description" content=""></meta>
			<meta name="author" content=""></meta>
			<title>Project</title>
			<!-- Bootstrap core CSS-->
			<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css"></link>
			<link rel="stylesheet" href="https://cdn.datatables.net/1.10.20/css/dataTables.bootstrap4.min.css"></link>
			<!-- Custom fonts for this template-->
			<link href="https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" rel="stylesheet" ></link>
			<link rel='stylesheet' href="/ledpax/static/src/css/ledhome.css"></link>
    	</head>
      	<body class="fixed-nav sticky-footer navbar-light" id="page-top">
            <section class="invoice_table">
                <div class="container"> 
                    <div class="row">
						<div class="cards col-lg-12 col-md-12 col-sm-12 mb-3">
							<div class="card-header prouduct-title"><i class="fa fa-table"></i>Project</div>
							<div class="row product-list-sec">
								<div class="col-xl-3 col-sm-6 mb-3">
									<div class="card text-white bg-danger o-hidden h-100">
										<div class="card-body">
											<div class="mr-5 text-center blockd">Quotes</div>
										</div>
										<a class="card-footer text-white clearfix small z-1" href="/my/project/quotes" name="project_qoutes" type="object">
											<span class="float-left">View Details</span>
											<span class="float-right">
												<i class="fa fa-angle-right"></i>
											</span>
										</a>
									</div>
								</div>
								<div class="col-xl-3 col-sm-6 mb-3">
									<div class="card text-white bg-warning o-hidden h-100">
										<div class="card-body">
											<div class="mr-5 text-center blockd">Invoices</div>
										</div>
										<a class="card-footer text-white clearfix small z-1" href="/my/project/invoices">
											<span class="float-left">View Details</span>
											<span class="float-right">
												<i class="fa fa-angle-right"></i>
											</span>
										</a>
									</div>
								</div>
								<div class="col-xl-3 col-sm-6 mb-3">
									<div class="card text-white bg-success o-hidden h-100">
										<div class="card-body">
											<div class="mr-5 text-center blockd">Documents</div>
										</div>
										<a class="card-footer text-white clearfix small z-1" href="/my/documents">
											<span class="float-left">View Details</span>
											<span class="float-right">
												<i class="fa fa-angle-right"></i>
											</span>
										</a>
									</div>
								</div>
								<div class="col-xl-3 col-sm-6 mb-3">
									<div class="card text-white bg-primary o-hidden h-100">
										<div class="card-body">
											<div class="mr-5 text-center blockd">Delivery Room</div>
										</div>
										<a class="card-footer text-white clearfix small z-1" href="/my/del_room">
											<span class="float-left">View Details</span>
											<span class="float-right">
												<i class="fa fa-angle-right"></i>
											</span>
										</a>
									</div>
								</div>
							</div>
						</div>
  					</div>
				</div>
			</section>
			<script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.min.js"></script>
			<script src="https://cdn.datatables.net/1.10.20/js/jquery.dataTables.min.js"></script>
			<script src="https://cdn.datatables.net/1.10.20/js/dataTables.bootstrap4.min.js"></script>
  			<script src="js/sb-admin.min.js"></script>
			<script>
				jQuery('#dataTable').DataTable();
			</script>
  		</body>
	</xpath>
	</t>
</template>
</odoo>