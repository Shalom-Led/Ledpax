<odoo>
<record id='product_list_view_inherit' model='ir.ui.view'>
    <field name="name">product.list.view.inherit</field>
    <field name="model">product.template</field>
    <field name="inherit_id" ref="product.product_template_only_form_view"/>
    <field name='arch' type='xml'>
  <xpath expr="//field[@name ='image_medium']" position='before' >
        <group>
           <field name='pdf_image' filename='file_pdf' />
           <field name='file_pdf' invisible='1'/>
        </group>
  </xpath>
  <xpath expr="//field[@name ='barcode']" position='after' >
          <field name='prod_variant' />
  </xpath>
  <xpath expr="//field[@name ='prod_variant']" position='after' >
          <field name='excel_file' filename='file_excel'/>
          <field name='file_excel' invisible='1' />
  </xpath>  
  </field>
</record>

<record id='product_list_view_inherit_prod' model='ir.ui.view'>
    <field name="name">product.list.view.inherit</field>
    <field name="model">product.product</field>
    <field name="inherit_id" ref="product.product_normal_form_view"/>
    <field name='arch' type='xml'>
  <xpath expr="//field[@name ='image_medium']" position='before' >
        <group>
           <field name='pdf_image' filename='file_pdf' />
           <field name='file_pdf' invisible='1'/>
        </group>
  </xpath>
  <xpath expr="//field[@name ='categ_id']" position='after' >
          <field name='excel_file' filename='file_excel'/>
          <field name='file_excel' invisible='1' />
  </xpath>  
  </field>
</record>

<menuitem id="import_product_excel" name="Import Products through Excel" action="action_product_create_excel_wizard" parent="sale.product_menu_catalog" sequence="8"/>

<menuitem id="import_product_image_excel" name="Import Products Images through Excel" action="action_product_image_create_excel_wizard" parent="sale.product_menu_catalog" sequence="9"/>

</odoo>